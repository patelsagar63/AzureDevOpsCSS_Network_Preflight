{
  "manifestVersion": 1,
  "id": "network-preflight",
  "publisher": "AzureDevOpsCSS-Sagar",
  "name": "Network Preflight (HTTP/DNS/TCP)",
  "version": "1.0.0",
  "description": "Azure Pipelines tasks to validate HTTP, DNS, and TCP connectivity from build/release agents before critical deployments.",
  "tags": [
    "Network",
    "Connectivity",
    "Preflight",
    "Azure Pipelines",
    "Azure DevOps"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "files": [
    { "path": "tasks" },
    { "path": "README.md" },
    { "path": "images" }
  ],
  "contributions": [
    {
      "id": "network-preflight-httpcheck",
      "type": "ms.vss-distributed-task.task",
      "targets": [ "ms.vss-distributed-task.tasks" ],
      "properties": {
        "name": "tasks/HttpCheckV1/task.json"
      }
    },
    {
      "id": "network-preflight-dnslookup",
      "type": "ms.vss-distributed-task.task",
      "targets": [ "ms.vss-distributed-task.tasks" ],
      "properties": {
        "name": "tasks/DnsLookupV1/task.json"
      }
    },
    {
      "id": "network-preflight-tcpprobe",
      "type": "ms.vss-distributed-task.task",
      "targets": [ "ms.vss-distributed-task.tasks" ],
      "properties": {
        "name": "tasks/TcpProbeV1/task.json"
      }
    }
  ]
}
